#include<stdio.h>
 int main() //adding n numbers
 {
      int n=10;
     int a[10]={9,8,8,0,3,8,9,2,4,6};
     printf("the sum of the numbers in the array is= %d \n", add(a,n));
     return 0;  
 }
  
 int add( int a[], int n )
 {
     int sum=0;
     for( int i=0;i<10; i++)
     {
         
         sum= sum+a[i];
     }
     return sum;
 }
